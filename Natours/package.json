{
  "name": "natours",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "concat:css": "concat -o starter/css/style.concat.css starter/css/icon-font.css starter/css/main.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' starter/css/style.concat.css -o  starter/css/style.prefix.css",
    "compress:css": "postcss starter/css/style.prefix.css > starter/css/style.css",
    "build:css": "npm-run-all concat:css prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concat": "^1.0.3",
    "cssnano": "^5.1.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "sass": "^1.54.0"
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "discardComments": {
          "removeAll": true
        }
      }
    ]
  }
}
